  "description": "Angular Webpack Demo Project",
  "main": "src/main/webapp/app/index.js",
  "scripts": {
    "start": "webpack-dev-server --TARGET=BUILD --content-base src/main/webapp",
    "dist": "webpack --TARGET=PROD",
    "test": "karma start --TARGET=TEST",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "validate": "npm-run-all --parallel lint test",
    "build": "webpack --TARGET=BUILD --display-chunks --display-reasons --display-error-details --display-modules",
    "build:prod": "webpack --TARGET=PROD",
    "lint": "eslint src/main/webapp/app/"
  },
